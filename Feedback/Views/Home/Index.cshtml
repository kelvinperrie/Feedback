            var feedbackDataUrl = "@Url.Action("AllFeedback", "Home")";
            console.log(feedbackDataUrl);
            return feedbackDataUrl;
        }

        function GetFeedbackVoteUp() {
            var asdf = "@Url.Action("VoteUpFeedback", "Home")";
            console.log(asdf);
            return asdf;
        }
        function GetFeedbackVoteDown() {
            var asdf = "@Url.Action("VoteDownFeedback", "Home")";
